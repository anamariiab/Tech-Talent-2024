
public class T4Ejercicio01App {

	public static void main(String[] args) {

	    // Declara dos variables numéricas (con el valor que desees)
		int numero1 = 15;
		int numero2 = 20;
		
		// Muestra por consola la suma
		int suma = numero1+numero2;
		System.out.println(suma);
		
		// Muestra por consola la resta
		int resta = numero1-numero2;
		System.out.println(resta);		
		
	    // Muestra por consola la multiplicación
		int multiplicacion = numero1*numero2;
		System.out.println(multiplicacion);
		
		// Muestra por consola la división
		int division = numero1/numero2;
		System.out.println(division);
		
		// Muestra por consola el modulo
		int modulo = numero1%numero2;
		System.out.println(modulo);
	}

}